{
    "extends": "airbnb/base",
    "env": {
        "node": true,
        "jasmine": true,
    },
    "rules": {
        'indent': [1, 4],
        'no-param-reassign': 0,
        'id-length': 0,
        'max-len': [2, 200, 2, {
             'ignoreUrls': true,
             'ignoreComments': false
         }],
         'object-shorthand': 0,
         "arrow-body-style": 0,
         "new-cap": 0,
    }
}
